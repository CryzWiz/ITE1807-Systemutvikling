 <div class="tab-pane" id="team">
     <div class="row">
       <div class="col-md-6 col-xs-12">
        <form class="form" id="teamForm">

            <div class="alert" role="alert" id="team-users-alert">
            </div>

            <div class="col-xs-12">
                <div class="alert alert-info alert-user" id="found_user-div" role="alert" style="display:none;">
                    <input type="hidden" id="emailToAdd" >
                    <input type="hidden" id="idToAdd" >
                    <input type="hidden" id="fullnameToAdd" >
                    <a href="#" class="btn btn-xs btn-primary pull-left" onclick="addFoundEmployeeToTeam()">add</a>
                    <strong>&nbsp;User:&nbsp;</strong><span id="found-user"></span>
                    <a href="#" class="close" onclick="$('.alert-user').hide()">&times;</a>
                </div>
                <div class="input-group">
                    <input type="text" class="form-control" id="find_username" placeholder="Search for employeee email...">
                    <span class="input-group-btn">
                        <button class="btn btn-default" type="button" id="find_employee">Finn!</button>
                    </span>
                </div><!-- /input-group -->
            </div><!-- /.col-xs-12 -->
            <div class="form-group">
                <div class="col-xs-12">

                    <label for="teams_leader">Select team:</label>
                    <select class="form-control" id="user_teams" {{ session.leader_in_teams_selval }}>
                    {% for key,value in session.leader_in_teams %}
                        <option value="{{ key }}"
                            {{  form.teams_leader.vars.value == '' and key == 0 ? ' selected ' :(val.value == form.teams_leader.vars.value ? ' selected ' : '') }}
                        >
                        {{ value }}
                        </option>
                    {% endfor %}
                    </select>
                </div>
            </div>
            <div class="form-group">

                <div class="col-xs-12" >
                    <div class="col-xs-12">Team medlemer:</div>
                    <ul class="list-group" id="team_members">
                    </ul>
                </div>
                <div class="form-group">
                    <div class="col-xs-12">
                        <button class="btn btn-md btn-success" type="button" onclick="submitTeamMembers()">
                            <i class="glyphicon glyphicon-ok-sign"></i> Lagre</button>
                    </div>
                </div>
            </div>
        </form>
         </div> <!-- div form1-->
         <div class="col-md-6 col-xs-12">
             <form class="form" action="#" method="post" id="taskForm">
                 {% if messageType2 != '' %}
                     <div class="alert alert-{{ messageType2 }}" role="alert">
                         {{ message2 }}
                     </div>
                 {% endif %}
                 <div class="form-group">
                     <div class="col-xs-12">
                         <label for="teams_leader">Select task:</label>
                         <select class="form-control" id="team_tasks" {{ session.leader_in_teams_selval }}>
                             {% for key,value in session.leader_in_teams %}
                                 <option value="{{ key }}"
                                         {{  form.teams_leader.vars.value == '' and key == 0 ? ' selected ' :(val.value == form.teams_leader.vars.value ? ' selected ' : '') }}
                                 >
                                     {{ value }}
                                 </option>
                             {% endfor %}
                         </select>
                     </div>
                 </div>
                 <div class="form-group">

                     <div class="col-xs-12">
                         <div class="col-xs-12">Team medlemer:</div>
                         <ul class="list-group" id="team_members">
                         </ul>

                     </div>
                     <div class="col-xs-12">
                         <button class="btn btn-md btn-info" type="button" ><i class="glyphicon glyphicon-ok-sign"></i> Find</button>

                     </div>

                     <div class="form-group">
                         <div class="col-xs-12">
                             <button class="btn btn-md btn-success" type="submit" name="submit" id="submit">
                                 <i class="glyphicon glyphicon-ok-sign"></i> Lagre</button>
                         </div>
                     </div>
                 </div>
             </form>
         </div> <!-- div form1-->
     </div> <!-- div row-->


     <div class="form-group" id="separator" >
         <div class="col-lg-12 col-md-12 col-s-12 col-xs-6">
             <span>&nbsp;</span>
         </div>
     </div>

     <!-- for postal lookup -->
     <script type="text/javascript" src="{{ assets_dir }}/js/profile_ajax.js"></script>
     <script type="text/javascript">
         onload();
     </script>
     <!-- for postal lookup -->
    </div><!--/tab-pane-->
